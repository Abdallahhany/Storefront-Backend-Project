{
  "name": "storefront_backend",
  "version": "1.0.0",
  "description": "",
  "main": "server.ts",
  "scripts": {
    "start": "node src/server.ts",
    "watch": "tsc-watch --esModuleInterop src/server.ts --outDir ./dist --onSuccess \"node ./dist/server.js\"",
    "migrate": "db-migrate --env test up && db-migrate up",
    "test1": "ENV=test db-migrate --env test up && jasmine && db-migrate db:drop test",
    "test": "ENV=test && db-migrate up --env test && tsc && jasmine && db-migrate reset",
    "jasmine": "jasmine",
    "migrationup": "db-migrate up",
    "migrationdown": "db-migrate down",
    "tsc": "tsc"
  },
  "author": "Udacity",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.20.0",
    "db-migrate": "^0.11.13",
    "db-migrate-pg": "^1.2.2",
    "dotenv": "^16.0.2",
    "express": "^4.18.1",
    "pg": "^8.8.0",
    "typescript": "^4.8.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/jasmine": "^4.3.0",
    "@types/pg": "^8.6.5",
    "jasmine": "^4.4.0",
    "jasmine-spec-reporter": "^7.0.0",
    "jasmine-ts": "^0.4.0",
    "ts-node": "^10.9.1",
    "tsc-watch": "^5.0.3"
  }
}
